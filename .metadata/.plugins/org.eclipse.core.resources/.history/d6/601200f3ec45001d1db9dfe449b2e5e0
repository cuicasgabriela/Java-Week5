
public class AsteriskLogger implements Logger {
	public void log(String str1) {
		
			System.out.println("***"+ str1 + "***");
	}

	public void error(String str2) {
		if(str2 == "Hello")
			System.out.println("ERROR: H e l l o");
		else {
		System.out.println("****************");
		System.out.println("***Error: "+ str2 + "***");
		System.out.println("****************");
		}
	}
}
