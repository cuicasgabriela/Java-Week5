
public class SpacedLogger implements Logger {

	public void log(String str1) {
		String str3 = "";
		for(int i = 0; i<str1.length(); i++) {
			str3 = str1.valueOf(i) + " ";
		}
		System.out.println(str3);
	}

	public void error(String str2) {
		String str3 = "";
		for(int i = 0; i<str2.length(); i++) {
			str3 = str2.valueOf(i) + " ";
		}
		
		System.out.print("ERROR: ");
		for(int i = 0; i<str2.length(); i++) {
			str3 = str2.valueOf(i) + " ";
		}
	}

}
